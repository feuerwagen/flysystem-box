{
    "name": "adnet/flysystem-box",
    "description": "Flysystem Adapter for the Box.com v2 API",
    "homepage": "https://gitlab.com/eugktech/flysystem-boxcom",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "flysystem-boxcom",
        "flysystem",
        "box.com",
        "v2",
        "api"
    ],
    "authors": [
        {
            "name": "Eugktech",
            "email": "work@eugk.name",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "league/flysystem": "^2.0|^3.0",
        "eugktech/boxcom-api": "dev-main"
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "require-dev": {
        "symfony/var-dumper": "*"
    },
    "autoload": {
        "psr-4": {
            "Adnet\\Flysystem\\Box\\": "src"
        }
    },
    "repositories": {
        "eugktech/boxcom-api": {
            "type": "vcs",
            "url": "https://gitlab.com/eugktech/boxcom-api"
        }
    }
}
